<launch>
    <node pkg="dvision"
          name="dvision_node"
          type="dvision_node"
          output="screen"
          respawn="false"
          required="true"
          cwd="node"
          args=""/>
    <rosparam command="load" file="$(find dconfig)/dvision/field_model.yml"/>
    <rosparam command="load" file="$(find dconfig)/dvision/camera.yml"/>
    <rosparam command="load" file="$(find dconfig)/dvision/localization.yml"/>
</launch>
